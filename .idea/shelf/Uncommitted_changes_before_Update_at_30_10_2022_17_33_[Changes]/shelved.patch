+package org.example;
+import java.util.Scanner;
+
+public class ConsoleBot {
+    public void botMain(){
+        Scanner in=new Scanner(System.in);
+        while(true){
+            String nmsg=in.nextLine();
+            printmsg(nmsg);
+        }
+    }
+    public void printmsg(String text){
+        System.out.println(text);
+    }
+
+}
